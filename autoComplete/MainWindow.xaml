<Window x:Class="autoComplete.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:autoComplete"
        xmlns:controls="clr-namespace:System.Windows.Controls;assembly=DotNetProjects.Input.Toolkit"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <TextBlock TextAlignment="Center" FontWeight="Bold" FontSize="20" Margin="0,10,0,20" Padding="10px">Auto Complete Text Box</TextBlock>
        <controls:AutoCompleteBox x:Name="tb"/>
        <Button Content="Button" Click="Button_Click" Margin="20"/>
    </StackPanel>
</Window>
